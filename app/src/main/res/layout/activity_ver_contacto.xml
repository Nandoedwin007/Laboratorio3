<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".VerContacto">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvNombre" android:layout_marginTop="184dp"
            app:layout_constraintTop_toTopOf="parent" android:textSize="30sp" android:layout_marginStart="128dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="128dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvTelefono" android:textSize="24sp"
            android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/tvNombre" android:layout_marginStart="140dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="140dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvEmail" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/tvTelefono" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="152dp" android:layout_marginStart="152dp" android:textSize="18sp"/>
    <Button
            android:text="Regresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnRegresar" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/tvEmail" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="144dp" android:layout_marginStart="144dp"/>
</android.support.constraint.ConstraintLayout>